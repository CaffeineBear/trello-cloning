{
  "name": "trello-cloning",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "run-script-os",
    "eslint:darwin:linux": "LIST=`git diff-index --name-only --diff-filter=d HEAD | grep .*\\.js | grep -v json`; if [ \"$LIST\" ]; then npx eslint $LIST; fi",
    "eslint:win32": "scripts\\eslint-windows.bat",
    "eslint-fix": "run-script-os",
    "eslint-fix:darwin:linux": "LIST=`git diff-index --name-only --diff-filter=d HEAD | grep .*\\.js | grep -v json`; if [ \"$LIST\" ]; then npx eslint --fix $LIST; fi",
    "eslint-fix:win32": "scripts\\eslint-windows.bat --fix",
    "eslint-all": "run-script-os",
    "eslint-all:darwin:linux": "npx eslint --no-error-on-unmatched-pattern src/**/*.js src/**/*.jsx src/*.js",
    "eslint-all:win32": "npx eslint --no-error-on-unmatched-pattern src\\**\\*.js src\\**\\*.jsx src\\*.js",
    "eslint-all-fix": "run-script-os",
    "eslint-all-fix:darwin:linux": "npx eslint --fix --no-error-on-unmatched-pattern src/**/*.js src/**/*.jsx src/*.js",
    "eslint-all-fix:win32": "npx eslint --fix --no-error-on-unmatched-pattern src\\**\\*.js src\\**\\*.jsx src\\*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "run-script-os": "^1.1.5"
  }
}
